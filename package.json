{
  "name": "vite-emojied-eval",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "gh:push": "gh-pages -d dist/ -m 'deploying to Pages'",
    "gh:publish": "npm run build && npm run gh:push",
    "sb:func:serve": "supabase start && supabase functions serve",
    "sb:status": "supabase status",
    "sb:projects": "supabase projects list",
    "sb:login": "supabase login",
    "sb:stop": "supabase stop",
    "sb:func:deploy": "supabase functions deploy",
    "sb:gen:ts": "npx supabase gen types typescript --local > src/supabase.types.ts"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@reactivex/rxjs": "^6.6.7",
    "@supabase/supabase-js": "^2.39.3",
    "connectkit": "^1.6.0",
    "emoji-mart": "^5.5.2",
    "gh-pages": "^6.1.1",
    "jose": "^5.2.0",
    "json5": "^2.2.3",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.21.2",
    "siwe": "^2.1.4",
    "viem": "^1.21.4",
    "wagmi": "^1.4.13"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "supabase": "^1.131.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
